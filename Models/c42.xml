    <!--  Rudder Animation  -->
    <animation>
        <type>rotate</type>
        <object-name>rudder</object-name>
        <property>/fdm/jsbsim/fcs/rudder-pos-deg</property>
        <factor>1</factor>
        <center>
            <x-m> 4.266</x-m>
            <y-m> 0.0</y-m>
            <z-m> 0.6047</z-m>
        </center>
        <axis>
            <x> 0.035</x>
            <y> 0.0</y>
            <z>-0.999</z>
        </axis>
    </animation>

    <!--  Elevator Animation  -->
    <animation>
        <type>rotate</type>
        <object-name>elevator</object-name>
        <object-name>trimtab</object-name>
        <property>/fdm/jsbsim/fcs/elevator-pos-deg</property>
        <factor>1</factor>
        <center>
            <x-m> 4.2165</x-m>
            <y-m> 0.00</y-m>
            <z-m>-0.017</z-m>
        </center>
        <axis>
            <x> 0.0</x>
            <y> 1.0</y>
            <z> 0.0</z>
        </axis>
    </animation>

