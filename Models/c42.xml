    <!-- Gear Animations -->
    <!-- Nose Gear -->
 <model>
  <path>Aircraft/Generic/Effects/tyre-smoke-nose.xml</path>
  <name>tyre-smoke-n</name>
  <offsets>
   <x-m>-0.856</x-m>
   <y-m>0</y-m>
   <z-m>-1.067</z-m>
  </offsets>
 </model>
    <animation>
        <type>rotate</type>
        <object-name>nosewheel</object-name>
        <object-name>nose.strut.piston</object-name>
        <property>/fdm/jsbsim/gear/unit/steering-angle-deg</property>
        <factor>-1</factor>
        <center>
            <x-m>-0.792</x-m>
            <y-m> 0.0</y-m>
            <z-m>-0.878</z-m>
        </center>
        <axis>
            <x> 0.31</x>
            <y> 0.0</y>
            <z> 0.95</z>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>nosewheel</object-name>
        <object-name>nose.axle</object-name>
        <object-name>nose.strut.piston</object-name>
        <property>gear/gear[0]/compression-ft</property>
        <factor>0.3048</factor>
        <center>
            <x-m>-0.856</x-m>
            <y-m> 0.</y-m>
            <z-m>-1.067</z-m>
        </center>
        <axis>
            <x>0.31</x>
            <y>0.0</y>
            <z>0.95</z>
        </axis>
    </animation>
    <animation>
        <type>spin</type>
        <object-name>nosewheel</object-name>
        <property>gear/gear[0]/rollspeed-ms</property>
        <factor>51.856</factor> <!-- 1 / Wheel circumference in m * 60 -->
        <center>
            <x-m>-0.856</x-m>
            <y-m> 0.</y-m>
            <z-m>-1.067</z-m>
        </center>
        <axis>
            <x> 0.0</x>
            <y>-1.0</y>
            <z> 0.0</z>
        </axis>
    </animation>

<!-- Left Main Gear --> 
 <model>
  <path>Aircraft/Generic/Effects/tyre-smoke-port.xml</path>
  <name>tyre-smoke-p</name>
