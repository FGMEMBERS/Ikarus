<?xml version="1.0"?>

<PropertyList>
<path>oil-temp.ac</path>

    <animation>
        <type>material</type>
        <object-name>needle</object-name>
        <object-name>face</object-name>
        <emission>
            <red>0.5</red>
            <green>0.35</green>
            <blue>0.2</blue>
            <factor-prop>/systems/electrical/outputs/instrument-lights-norm</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>needle</object-name>
        <property>engines/engine/oil-temperature-degf</property>
        <interpolation>
            <entry><ind>  0.0</ind><dep> 0.0</dep></entry>
            <entry><ind>122.0</ind><dep> 7.5</dep></entry>
            <entry><ind>176.0</ind><dep>21.0</dep></entry>
            <entry><ind>212.0</ind><dep>40.5</dep></entry>
            <entry><ind>248.0</ind><dep>71.5</dep></entry>
            <entry><ind>266.0</ind><dep>84.0</dep></entry>
            <entry><ind>302.0</ind><dep>98.5</dep></entry>
         </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m> 0</x-m>
            <y-m> 0.0107</y-m>
            <z-m> 0.0003</z-m>
        </center>
    </animation>
<!-- Normalmap effect -->
    <effect>
      <inherits-from>Aircraft/Ikarus/Models/Effects/Ikarus-instr-bump</inherits-from>
      <object-name>face</object-name>
      <object-name>bezel</object-name>
     </effect>
</PropertyList>
